#import "XBJInfoView.h"
#import "Masonry.h"

@implementation XBJInfoView


- (void)setFrame:(CGRect)frame{
    [super setFrame:frame];
    self.type = MMPopupTypeCustom;
    [self mas_makeConstraints:^(MASConstraintMaker *make) {
        make.size.mas_equalTo(frame.size);
    }];    
}

